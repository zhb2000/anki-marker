{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:allow-message",
    "dialog:allow-confirm",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*"
        },
        {
          "url": "https://*"
        },
        {
          "url": "http://*:*"
        },
        {
          "url": "https://*:*"
        }
      ]
    },
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "clipboard-manager:allow-read-text",
    "core:app:allow-app-show",
    "core:app:allow-app-hide",
    "shell:default",
    "http:default",
    "dialog:default",
    "os:default",
    "clipboard-manager:default"
  ]
}
